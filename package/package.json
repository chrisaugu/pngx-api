{
  "name": "@chrisaugu/nuku-api-lib",
  "version": "1.0.0",
  "description": "NUKU API NPM library",
  "typex": "commonjs",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "_clear": "rimraf build/compiled/*",
    "_tsc": "tsc",
    "_make-bundle": "rollup --config",
    "buildxx": "run-s _clear _tsc _make-bundle",
    "prebuild": "node scripts/incremental-build.js",
    "buildx": "run-s lint type test build:clean npx rollup -c",
    "build": "rollup -c",
    "jest": "jest --watch",
    "test": "run-s test:unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs.js"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisaugu/PNGX-API.git"
  },
  "author": "Christian Augustyn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisaugu/PNGX-API/issues"
  },
  "homepage": "https://github.com/chrisaugu/PNGX-API#readme",
  "dependencies": {
    "@chrisaugu/nuku-api-lib": "^1.0.0",
    "csv-parse": "^5.6.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "keywords": []
}